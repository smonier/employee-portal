{
  "name": "employee-portal",
  "version": "0.1.1",
  "type": "module",
  "files": [
    "dist",
    "src/**/*.cnd",
    "settings",
    "static"
  ],
  "scripts": {
    "build": "tsc --noEmit && vite build && run package",
    "clean": "rm -rf dist/",
    "deploy": "jahia-deploy",
    "format": "prettier --write --list-different .",
    "dev": "vite build --watch",
    "lint": "eslint .",
    "package": "yarn pack --out dist/package.tgz",
    "watch": "run dev",
    "watch:callback": "run package && run deploy"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "lucide-react": "^0.548.0",
    "modern-normalize": "^3.0.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.49.0",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.26.0",
    "@jahia/javascript-modules-library": "^1.0.1",
    "@jahia/vite-plugin": "^1.0.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19",
    "cross-env": "^10.0.0",
    "eslint": "^9.26.0",
    "globals": "^16.0.0",
    "i18next": "^25.0.2",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-i18next": "^15.5.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^7.0.0"
  },
  "packageManager": "yarn@4.9.4",
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">=4.0.0"
  },
  "jahia": {
    "snapshot": true,
    "module-dependencies": "default",
    "module-type": "templatesSet",
    "server": "dist/server/index.js",
    "static-resources": "/dist/client,/dist/assets,/locales,/images,/icons,/static"
  }
}
